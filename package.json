{
  "name": "atomic-component-library",
  "private": false,
  "version": "0.0.2",
  "main": "./dist/atomic-component-library.umd.js",
  "module": "./dist/atomic-component-library.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/atomic-component-library.es.js",
      "require": "./dist/atomic-component-library.umd.js"
    }
  },
  "engines": {
    "node": ">=16.18.1"
  },
  "scripts": {
    "dev": "start-storybook -p 6006",
    "build": "build-storybook",
    "build:lib": "vite build",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx --fix",
    "format:prettier": "prettier --write \"src/**/*.{ts,tsx,js,mdx}\"",
    "test": "vitest",
    "test:cov": "vitest run --coverage",
    "prepare": "husky install",
    "prepack": "json -f package.json -I -e \"delete this.devDependencies; delete this.dependencies\""
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "files": [
    "dist"
  ],
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,css,md}": [
      "prettier -w"
    ]
  },
  "description": "## Features",
  "author": "",
  "license": "ISC"
}
